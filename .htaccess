RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Redirect to .php
RewriteRule ^([^\.]+)$ $1.php [NC,L]

#Redirect /home to index.php
RewriteRule ^home index.php [NC,L]

RewriteRule ^quote/([0-9a-zA-Z_-]+) view-quote.php?ref=$1 [L]
RewriteRule ^download-quote/([0-9a-zA-Z_-]+) download-quote.php?ref=$1 [L]

